// Test 6: If-else conditionals
int main() {
    int x = 10;

    if (x > 5) {
        printf("x is greater than 5\n");
    }

    if (x < 5) {
        printf("x is less than 5\n");
    } else {
        printf("x is not less than 5\n");
    }

    if (x < 0) {
        printf("x is negative\n");
    } else if (x == 0) {
        printf("x is zero\n");
    } else {
        printf("x is positive\n");
    }

    return 0;
}
