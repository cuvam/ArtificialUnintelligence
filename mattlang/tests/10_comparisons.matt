// Test 10: Comparison operators
int main() {
    int a = 10;
    int b = 20;

    printf("a = %d, b = %d\n", a, b);

    if (a == b) {
        printf("a == b\n");
    }

    if (a != b) {
        printf("a != b\n");
    }

    if (a < b) {
        printf("a < b\n");
    }

    if (a > b) {
        printf("a > b\n");
    } else {
        printf("a is not greater than b\n");
    }

    if (a <= b) {
        printf("a <= b\n");
    }

    if (a >= b) {
        printf("a >= b\n");
    } else {
        printf("a is not >= b\n");
    }

    return 0;
}
